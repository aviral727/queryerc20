{
    "name": "StakinAPI",
    "version": 2,
    "build": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    
    "routes": [
        {
            "src": "/get",
            "dest": "/index.js"
        },
        {
            "src": "/set/([^/]+)",
            "dest": "/index.js"
        }
    ]
}
